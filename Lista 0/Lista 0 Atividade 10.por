programa {
  funcao inicio() {

    //Declarar variveis

    var real: RENDIMENTO, CAPITAL,TEMPO

    //Dados

    escreva ("Informe o capital que deseja aplicar: ")
    leia (CAPITAL)

    escreva ("Informe o período do rendimento: ")
    leia (TEMPO)

    RENDIMENTO = CAPITAL*TEMPO*0.70

    //Resultado

    escreva ("Após ", TEMPO, " meses seu dinheiro terá rendido: ", RENDIMENTO)
    escreva (" R$")
  }
}
