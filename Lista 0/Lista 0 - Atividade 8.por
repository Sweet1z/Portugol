programa {
  funcao inicio() {
    //declarar variaveis
    
    var real: C, MULTIPLICACAO, DIVISAO, SOMA

    //dados

    escreva("Digite a temperatura em Celsius: ")
    leia (C)
    
    MULTIPLICACAO = (9*C)
    SOMA = (MULTIPLICACAO+160)
    DIVISAO = (SOMA/5)

    // resultados

    escreva ("A temperatura em Fahrenheit é ", DIVISAO)
  }
}
