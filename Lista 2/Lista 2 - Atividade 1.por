programa {
  funcao inicio() {

    cadeia NASCIMENTO, NOME
    real IDADE

   escreva("Escreva seu nome: ")
   leia(NOME)
   
   escreva("Digite a sua idade: ")
   leia(IDADE)

   se 
   (IDADE >= 18){
    escreva (NOME, "Sua entrada é permitida.")
   }
    senao 
    escreva (NOME, "Sua entrada não é permitda.")
  }
}
 